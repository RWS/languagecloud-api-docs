{
  "metadata": [
    {
      "src": [
        {
          "src": "resources/bcm/assembly/v2.1.3",
          "files": [
            "Sdl.Core.Bcm.BcmModel.dll"
          ]
        }
      ],
      "dest": "api/bcm",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.json",
          "**/*.html",
          "**/*.js"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "filemetadata": {
      "langs": {
        "api/**.yml": [
          "csharp",
          "vb",
          "fsharp",
          "cpp"
        ]
      }
    },
    "template": [
      "RWSTemplate"
    ],
    "postProcessors": [
      "ExtractSearchIndex",
      "EnvironmentVariableProcessor"
    ],
    "globalMetadata": {
      "_enableSearch": true
    },
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}