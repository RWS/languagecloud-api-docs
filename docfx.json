{
  "metadata": [
    {
      "src": [
        {
          "src": "articles/BCM/Assembly/v2.0.0",
          "files": [
            "Sdl.Core.Bcm.BcmModel.dll"
          ]
        }
      ],
      "dest": "api/BCM",
      "disableGitFeatures": true,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      { 
        "files": [
          "apiconcepts/**.md",
          "apiconcepts/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "filemetadata":
    { 
      "langs": {
      "api/**.yml": ["csharp", "vb", "fsharp", "cpp"]
      }
    },
    "template": [
      "RWSDocsTemplate"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}